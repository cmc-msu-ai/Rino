call_num	(\w*звон\w+)\s([^\d]+\s)*(\+?[\d\s]+)	~	null,null,number
call_num	(\w*звон\w+)\s(((\w+)\s){0,2}(\w+))	~	null,contact
dial_num	(набер\w+|набрать|набир\w+)\s(\+?[\d\s]+)	~	null,number
run_ussd	(набер\w+|набрать|набир\w+)\s((\*|звездочка\s)(\d+)\s?(#|решетка))	~	null,null,null,number,null
balance		((\w+\s)?баланс)|((\w+\s)?сколько денег\s(\w+\s)?на\s(\w+\s)?телефоне)
web_page	откр\w+\s((\w+.)+(ru|com|org|net|su))	~	webpage
send_email	отправ\w+\s(письмо|email)\s(\w+\s)*([^S]+@[^S]+)\s((\w+)*)	~	null,null,email,text
send_sms	отправ\w+\s(sms|смс)\s([^\d]+((\+)?[\d\s]+))\s((\w+)*)	~	null,number,null,null,text
set_alarm	(\w+\s)?будильник\s(на\s)?(\d+)(\sчас\w*)?\s(\d+)(\sминут\w*)?	~	null,null,hours,null,minutes,null
find		(найди|найти)\s((\w+\s)*(\w+))	~	null,contact