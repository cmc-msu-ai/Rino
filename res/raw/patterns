call_num	(\w*звон\w+) (\+?[\d\s]+)	~	number1
dial_num	(набер\w+|набрать|набир\w+) (\+?[\d\s]+)	~	number1
run_ussd	(набер\w+|набрать|набир\w+) ((\*|звездочка )(\d+) ?(#|решетка))	~	number1
balance		((\w+ )?баланс)|((\w+ )?сколько денег (\w+ )?на (\w+ )?телефоне)
web_page	откр\w+ (\w+).(ru|com|org|net|su)
send_email	отправ\w+ (письмо|email) (\w+ )*([^S]+@[^S]+)
send_sms	отправ\w+ (sms|смс)[^\d]+((\+)?[\d ]+)